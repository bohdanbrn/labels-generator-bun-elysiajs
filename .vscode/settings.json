{
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": false
    },
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.lintTask.enable": true,
    "cSpell.logLevel": "Information",
    "cSpell.ignoreWords": [
        "elysia",
        "Elysia",
        "elysiajs",
        "tsbuildinfo",
        "codepaths",
        "Docxtemplater",
        "pizzip",
        "linebreaks",
        "nodebuffer"
    ]
}
